{
    "configurations": [
        {
            "name": "Launch Chrome",
            "request": "launch",
            "type": "chrome",
            "url": "http://localhost:8080",
            "webRoot": "${workspaceFolder}"
        },

    {
        "name": "Launch Edge",
        "request": "launch",
        "type": "msedge",
        "url": "${workspaceFolder}/index.html",
        "webRoot": "${workspaceFolder}"
    }
{
            "name": "Attach to Edge",
            "port": 9222,
            "request": "attach",
            "type": "msedge",
            "webRoot": "${workspaceFolder}"
        },
    ]
}